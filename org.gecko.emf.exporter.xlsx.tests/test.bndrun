
-runfw: org.apache.felix.framework;version='[7.0.1,7.0.1]'
-runprovidedcapabilities: ${native_capability}

-resolve.effective: active

-runbundles.junit5: ${test.runbundles}

-runee: JavaSE-17

-runtrace: true

-runproperties.debug: \
	felix.log.level=4,\
	org.osgi.service.log.admin.loglevel=DEBUG

-runproperties.poi: \
	org.osgi.framework.bootdelegation=org.w3c.dom

-runproperties.trees: \
	TREES_DATASET_XMI="${.}/testdata/Trees/CityTree.xmi"

-runrequires: \
	bnd.identity;id='org.gecko.emf.exporter.xlsx',\
	bnd.identity;id='org.gecko.emf.exporter.xlsx.tests'

