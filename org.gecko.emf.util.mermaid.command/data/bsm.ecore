  <eClassifiers xsi:type="ecore:EClass" name="StrukturObjekt">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Eine räumlich und zeitlich abgrenzbare Aktion eines Trägers."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="parent" eType="#//StrukturObjekt">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Verweis auf die übergeordnete Massnahme."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="uuid" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Eindeutlige und unveränderliche ID der Massnahnme."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="traeger" lowerBound="1"
        eType="#//Traeger"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="zeitraum" lowerBound="1"
        upperBound="-1" eType="#//Zeitraum">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Verweis aus die Massnahme des Zeitraums"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="bezeichnung" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Kurzer schnell verständlicher Name der Massnahme, der in der Regel auf jedem offiziellen Dokument verwendet wird.&#xA;Für Straßenbaumaßnahmen idealerweise im Muster Straßenname: Art d. Maßnahme.&#xA;"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="beschreibung" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Text, der hilft, der öffentlichkeit die Masnahme näher zu erklären. Länge des textes sollte 500 zeichen nicht überschreiten."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="externe_id" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Id im Quellsystem dieses Datensatzes."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="parkplatzBetroffen" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"
        defaultValueLiteral="false">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Gibt an, ob die Baumassnahme zu Einschänkungen an Parkplätzen führt."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Traeger">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="title" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Eine kurze übersichtliuche Bezeichnung des Trägers."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="ZeitraumTyp">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Liste an Markern, die verwendet werden muss um Zeiträume zu klassifizieren."/>
    </eAnnotations>
    <eLiterals name="IDEE_TRAEGER">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Markiert den Zeitraum, den sich der Täger initial und unamgestimmt Vorschlägt. Idee des Trägers&#xA;"/>
      </eAnnotations>
    </eLiterals>
    <eLiterals name="KOORDINIERT_BSM" value="1">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Markiert den Zeitraum, der nach Abstimmung innerhalb des Baustellenmanagements festgelegt wurde."/>
      </eAnnotations>
    </eLiterals>
    <eLiterals name="BEAUFTRAGT" value="2">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Markiert den Zeitraum, in dem das Unternehmen mit der Umsetzung beauftragt ist."/>
      </eAnnotations>
    </eLiterals>
    <eLiterals name="ZWANG" value="3">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Markiert einen Zeitraum in dem die Massnahme zwingend erledigt erden muss. Kann gleichzeitig zum VORSSCHLAG_TRAEGER erstellt werden.&#xA;"/>
      </eAnnotations>
    </eLiterals>
    <eLiterals name="FREIGABE_STRASSE" value="4">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Der Zeitraum für die die Baustelle eine Freigabe der Straßen/Verkehrsbehörde hat.&#xA;"/>
      </eAnnotations>
    </eLiterals>
    <eLiterals name="IST" value="5">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Tatsächlicher Zeitpunkt der realen Ausführung."/>
      </eAnnotations>
    </eLiterals>
    <eLiterals name="PLANUNG" value="6">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Markiert den jeweils genausten Zeitraum vor IST"/>
      </eAnnotations>
    </eLiterals>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Zeitraum">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Historisierter Zeitraum einer Massnahme. Genauigkeit von 'start' und 'ende' ist abhängig von dem Horizint der Umsetzung&#xA;- >4 Jahre mindestens jährlich&#xA;- 2-4 Jahre mindestens halbjährlich&#xA;- &lt;2 Jahre monatsscharf&#xA;&#xA;Gültig ist,&#xA;- je Typ, immer der Zeitraum mit dem spätesten 'erstellt' , der keine 'aufgehoben' hat.&#xA;- typübergreifend, wie 'je Typ'  und dann in folgender Rangordnung der Zeitraumtypen: &#xA;> ZWANG&#xA;> IDEE_TRAEGER&#xA;> KOORDINIERT_BSM&#xA;> BEAUFTRAGT&#xA;> FREIGABE_STRASSE&#xA;> PLANUNG&#xA;> IST&#xA;"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="erstellt" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDate"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="start" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDate">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Markiert den in der Realität stattfindenden ZeitraumStart."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="ende" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDate">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Markiert den in der Realität stattfindenden ZeitraumEnde.&#xA;Nur im IST ist das Ende Optional"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="typ" lowerBound="1" eType="#//ZeitraumTyp">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Markiert die art des Zeitraumes"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="aufgehoben" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDate">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Datum, an dem dieser Zeitraum als als aufgehoben markiert wird."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="hinweis" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Hinweis darauf, wieso ein neuer Zeitabschnitt erstellt wird."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="aenderungsTyp" eType="#//ZeitraumAenderungsTyp">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Typisiete Begründung für die Änderung des vorhergehenden Zeitraum."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
