    <eLiterals name="INVESTPLAN" value="1">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Fokus mittelfristige Investitionen"/>
      </eAnnotations>
    </eLiterals>
