    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Baumassnahme eines Trägers."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="the_geom" eType="#//PolygonGeom">
